{
    "$schema": "https://gofastmcp.com/public/schemas/fastmcp.json/v1.json",
    "source": {
      "path": "main.py",
      "entrypoint": "mcp"
    },
    "environment": {
      "type": "uv",
      "python": "3.14",
      "dependencies": ["fastmcp[dev]", "coloraide>=5.1"]
    },
    "deployment": {
      "transport": "http",
      "host": "127.0.0.1",
      "port": 8000,
      "log_level": "DEBUG",
      "env": {
        "DEBUG": "true",
        "ENV": "development"
      }
    }
  }